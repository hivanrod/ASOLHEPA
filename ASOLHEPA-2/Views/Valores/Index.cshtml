@model IEnumerable<ASOLHEPA_2.Valore>

@{
    ViewBag.Title = "Valores";
}
<table class="tb2">
    <tr>
        <td>
            <h2>Listado de Valores de Asociado</h2>
        </td>
        <td class="SideL">
            @Html.ActionLink("Crear Nuevo Valor de Asociado +", "Create", "", new { @class = "LNK1" })
        </td>
    </tr>
    <tr>
        <td colspan="2">
            @TempData["Message"]
        </td>
    </tr>
</table>




<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Fecha)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdAutorización)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IdDonación)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OtrosValores)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TotalCobro)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Observaciones)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Asociado.Nombres)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Fecha)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdAutorización)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdDonación)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OtrosValores)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TotalCobro)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Observaciones)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Asociado.Nombres)
            </td>
            <td>
                @Html.ActionLink("Edita", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Detalles", "Details", new { id = item.Id }) |
                @Html.ActionLink("Borrar", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
