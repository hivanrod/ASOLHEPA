@model IEnumerable<ASOLHEPA_2.ServiciosEspecializadosInstitucione>

@{
    ViewBag.Title = "Listado de Servicios Especializados de Instituciones de Salud";
}

<table class="tb2">
    <tr>
        <td>
            <h2>istado de Servicios Especializados de Instituciones de Salud</h2>
        </td>
        <td class="SideL">
            @Html.ActionLink("Crear Nuevo Servicio Especializado de Instituciones de Salud +", "Create", "", new { @class = "LNK1" })
        </td>
    </tr>
    <tr>
        <td colspan="2">
            @TempData["Message"]
        </td>
    </tr>
</table>



<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Servicio)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Servicio)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
                @Html.ActionLink("Details", "Details", new { id = item.Id }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.Id })
            </td>
        </tr>
    }

</table>
