<!DOCTYPE html>
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - BASE DE DATOS - INTRANET - ASOLHEPA.ORG</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<style type='text/css'>
    tr:nth-child(even) {
        background-color: rgba(220, 219, 219, 0.39);
    }
</style>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("BD.ASOLHEPA.org", "Index", "Home", new { area = "" }, new { @class = "navbar-brand" })
            </div>
            <div class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li>@Html.ActionLink("Asociados", "Index", "Asociados")</li>
                    <li>@Html.ActionLink("EntidadesSalud", "Index", "EntidadesSalud")</li>
                    <li>@Html.ActionLink("Especialistas", "Index", "Especialistas")</li>
                    <li>@Html.ActionLink("InstitucionesSalud", "Index", "InstitucionesSalud")</li>
                    <li>@Html.ActionLink("Procedimientos", "Index", "Procedimientos")</li>
                    <li>@Html.ActionLink("Autorizaciones", "Index", "Autorizaciones")</li>
                    <li>@Html.ActionLink("Donaciones", "Index", "Donaciones")</li>
                    <li>@Html.ActionLink("Valores", "Index", "Valores")</li>
                </ul>
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - BD.ASOLHEPA.ORG</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
